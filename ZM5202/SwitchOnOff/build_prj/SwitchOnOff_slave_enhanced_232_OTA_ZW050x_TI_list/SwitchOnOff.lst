  72      =2   *    pin     - Z-Wave pin name
  73      =2   */
  74      =2   /*Fix TO#02731*/
  75      =2  #define PIN_ON(pin)  {pin##SHADOW |= (1<<pin);pin##Port = pin##SHADOW;}
  76      =2  #define PIN_HIGH(pin) {pin##SHADOW |= (1<<pin);pin##Port = pin##SHADOW;}
  77      =2  
