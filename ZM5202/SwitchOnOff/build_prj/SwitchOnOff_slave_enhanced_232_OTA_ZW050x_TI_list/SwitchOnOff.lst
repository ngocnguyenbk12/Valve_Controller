  69      =3  ** BYTE repeats);    IN  Number of function calls (-1: forever)
  70      =3  **--------------------------------------------------------------------------*/
  71      =3  #define ZW_TIMER_START(func,ticks,repeats) TimerStart(func,ticks,repeats)
