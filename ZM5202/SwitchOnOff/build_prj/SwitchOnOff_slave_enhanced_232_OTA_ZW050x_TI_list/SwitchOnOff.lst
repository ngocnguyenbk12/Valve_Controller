  60      =2   */
  61      =2  #if defined (ZW040x) || defined(ZW050x)
  62      =2  #define PIN_OUT(pin)  {pin##DIR_PAGE;(pin##SHADOWDIR &=~(1<<pin)) ;(pin##DIR = pin##SHADOWDIR);}
  63      =2  #endif /* #if defined (ZW040x) || defined(ZW050x) */
  64      =2  
